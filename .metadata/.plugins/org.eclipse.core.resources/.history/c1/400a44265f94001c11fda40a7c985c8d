package p7;

public class Sample1 {

	void sayHello() {

		System.out.println("Hello");
	}

	static void sayBye() {

		System.out.println("Bye, TC...");
	}

	public static void main(String[] args) {

		// call sayHello()
		Sample1 obj = new Sample1();
		obj.sayHello();
		obj.sayBye();

		// call sayBye()
		Sample1.sayBye();

	}

}
